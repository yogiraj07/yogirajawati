		header=new Node(0);
		listSize=0;
	}
    public void addNode(int data)
    {
    	
    }
    
	public static void main(String[] args) {

	}

}

class Node{
	private int data;
	private Node next;
	
	
	public Node(int data) {
		super();
		this.data = data;
		this.next = null;
	}
	public int getData() {
		return data;
	}
	public void setData(int data) {
		this.data = data;
	}
	public Node getNext() {
		return next;
	}
	public void setNext(Node next) {
		this.next = next;
	}
	
}